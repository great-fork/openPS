{
  "name": "openPS",
  "version": "0.4.3",
  "main": "public/electron.js",
  "author": {
    "name": "Geekeren",
    "url": "http://wangbaiyuan.cn",
    "email": "wangbaiyuan@outlook.com"
  },
  "description": "A lucky draw tool based on React, 基于React的炫酷的滚动抽奖程序",
  "repository": {
    "type": "git",
    "url": "https://github.com/geekeren/Magpie-LuckyDraw.git"
  },
  "bugs": {
    "url": "https://github.com/geekeren/Magpie-LuckyDraw/issues"
  },
  "homepage": "./",
  "dependencies": {
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "release-it": "^12.3.0",
    "wait-on": "^3.2.0"
  },
  "scripts": {

    "start": "electron .",
    "electron-pack": "build -c.extraMetadata.main=public/electron.js",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist:app": "rm -rf dist && electron-builder -mwl",
    "dist": "yarn dist:web && yarn dist:app",
    "release": "release-it"
  },
  "bin": {
    "magpie": "./cli.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
